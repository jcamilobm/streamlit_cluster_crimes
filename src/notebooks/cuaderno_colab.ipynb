        "# Se calcula la densidad poblacional promedio de cada comuna\n",
        "df_model['poblacional_km2'] = df_model['personas'] / df_model['area']\n",
        "\n",
        "df_model[\"manzanas_km2\"] = df_model[\"manzanas\"] / df_model[\"area\"]"
